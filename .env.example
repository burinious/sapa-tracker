VITE_FIREBASE_API_KEY=your_firebase_web_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# SAPA A.I mode (free/local by default)
VITE_SAPA_AI_PREMIUM_ENABLED=false
VITE_OPENAI_ENDPOINT=http://localhost:8787/api/sapa-ai
VITE_OPENAI_MODEL=gpt-5-mini

# Backend-only variables (set in server environment, not in frontend build env):
# OPENAI_API_KEY=sk-...
# OPENAI_MODEL=gpt-5-mini
# AI_BACKEND_PORT=8787
# AI_BACKEND_ALLOW_ORIGINS=http://localhost:5173,capacitor://localhost,http://localhost
# FIREBASE_WEB_API_KEY=your_firebase_web_api_key
# AI_BACKEND_RATE_LIMIT_MAX=30
# AI_BACKEND_RATE_LIMIT_WINDOW_MS=60000
